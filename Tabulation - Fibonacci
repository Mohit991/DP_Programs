import java.util.*;
class Main {
    public static int fib(int number){
        int fib[] = new int[number+1];
        fib[0] = 0;
        fib[1] = 1;
        for(int i = 2; i<= number; i++){
            fib[i] = fib[i-1] + fib[i-2];
        }
        return fib[number];
    }
    
    public static void main(String[] args) {
      System.out.println(fib(30));
    }
}
